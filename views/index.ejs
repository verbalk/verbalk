<%- partial('layout/headlayout.ejs') %>
<head>
  <script src="https://code.jquery.com/jquery-1.10.2.min.js"></script>
  <script src="js/main.js"></script>
  <link rel='stylesheet' href='/stylesheets/socket.css' />
</head>
<body>
  	<div id='top-container'>
		<nav id='top-space' >
		    <ul id='menu' class='clearfix-after'>
		        <li class='menu-cell'><a class='menu' href='/logout_user' style='color:#666;'>Logout</a></li>
		    </ul>
		</nav>
	</div>
	<div id='middle-space'>
		<ul class="pages">
			<li class="chat page">
			  <div class="chatArea">
			    <ul class="messages"></ul>
			    <input class="inputMessage" placeholder="Type here..."/>
			  </div>
			</li>
			
			<li class="login page">
			  <div class="form">
			    <h3 class="title">What's your nickname?</h3>
			    <input class="usernameInput" type="text" maxlength="14" />
			  </div>
			</li>
		</ul>
	</div>
</body>
